Proceso ejercicio20
	Definir num1, num2 Como Entero;
	
	Escribir "Ingrese el primero numero";
	Leer num1;
	
	Escribir "Ingrese el segundo numero";
	Leer num2;
	
	Si (num1 > num2)  Entonces
		Escribir "El numero ", num1, " es mayor";
	SiNo
		Escribir "El numero", num2, "es mayor";
	FinSi
FinProceso
