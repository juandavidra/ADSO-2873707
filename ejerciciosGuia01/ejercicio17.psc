Proceso ejercicio17
	Definir num1, num2 Como Real;
	
	Escribir "Ingrese el valor del numero 1";
	Leer num1;
	
	Escribir "Ingrese el valor del numero 2";
	Leer num2;
	
	Si num1 == num2 Entonces
		Escribir "Los numeros ingresados son iguales";
	SiNo
		Escribir "Los numeros ingresados no son iguales";
	FinSi
	
