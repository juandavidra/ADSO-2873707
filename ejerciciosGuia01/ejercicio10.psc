Proceso ejercicio10
	Definir alto, ancho, perimetro, area Como Real;
	
	Escribir "Ingrese el alto del rectangulo";
	Leer  alto;
	
	Escribir "Ingrese el ancho del rectangulo";
	Leer ancho;
	
	perimetro <- (alto + ancho) * 2;
	area <- alto * ancho;
	
	Escribir "El area del rectangulo es ",area, " y el perimetro es ", perimetro;
	
FinProceso
